{
  "name": "generic-hypermedia-app",
  "version": "1.1.4",
  "main": "src/index.js",
  "license": "MIT",
  "author": "LasaleFamine <info@godev.space>",
  "repository": "PolymerX/polymer-skeleton",
  "contributors": [
    "Alessio Occhipinti <info@godev.space> (https://godev.space)",
    "Mattia Astorino (http://equinsuocha.io/)"
  ],
  "scripts": {
    "build": "tsc; webpack",
    "build:prod": "tsc; webpack --env.NODE_ENV=production --optimize-minimize",
    "build:stats": "tsc; webpack --env production --json > stats.json",
    "dev": "tsc -w & webpack-dev-server --hot --inline",
    "pretest": "yarn build",
    "test": "yarn lint && wct --npm && yarn test:lighthouse",
    "test:lighthouse": "concurrently --kill-others \"http-server dist\" \"lighthouse --view http://localhost:8080\" ",
    "test:travis": "yarn pretest && yarn lint && wct --npm",
    "clean:lighthouse": "rm *.report.html",
    "posttest": "yarn rmbower",
    "lint": "tslint --project tsconfig.json && xo && stylelint src/components/**/*.pcss",
    "release": "standard-version",
    "start": "yarn build && http-server dist"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "eol-last": 0,
      "import/no-unassigned-import": 0,
      "promise/prefer-await-to-then": 0
    }
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@types/lodash.flatten": "4.4.4",
    "autoprefixer": "9.6.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "1.0.0",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "4.6.0",
    "cssnano": "4.1.7",
    "ejs-loader": "0.3.1",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "lighthouse": "3.2.1",
    "postcss": "7.0.6",
    "postcss-cssnext": "3.1.0",
    "postcss-custom-media": "7.0.8",
    "postcss-discard-comments": "4.0.1",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.0",
    "postcss-nesting": "7.0.0",
    "postcss-reporter": "6.0.0",
    "postcss-selector-not": "4.0.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "standard": "12.0.1",
    "standard-version": "4.4.0",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "terser-webpack-plugin": "1.1.0",
    "text-loader": "0.0.1",
    "tslint": "5.11.0",
    "typescript": "3.2.2",
    "webpack": "4.27.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "workbox-webpack-plugin": "3.6.3",
    "xo": "0.23.0"
  },
  "dependencies": {
    "@hydrofoil/alcaeus-loader": "^0.3.1",
    "@hydrofoil/hydrofoil-shell": "^0.2.2",
    "@hydrofoil/hydrofoil-paper-shell": "^0.2.0",
    "@lit-any/components-paper-elements": "^0.9.0",
    "@lit-any/forms": "^0.9.1",
    "@lit-any/views": "^0.9.0",
    "@polymer/decorators": "3.0.0",
    "@polymer/paper-card": "3.0.1",
    "@polymer/paper-dropdown-menu": "3.0.1",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/paper-styles": "3.0.1",
    "@polymer/paper-tabs": "3.0.1",
    "@polymer/paper-toast": "3.0.1",
    "@polymer/paper-tooltip": "3.0.1",
    "@polymer/polymer": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.0",
    "alcaeus": "^0.6.2",
    "feather-icon-literals": "1.0.0-rc.6",
    "ld-navigation": "^0.5.0",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0",
    "lodash.flatten": "4.4.0"
  }
}
